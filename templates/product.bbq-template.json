        "a9b9159f-7fe2-4149-bf31-0833258247c1": {
          "type": "custom",
          "settings": {
            "code": "<div class=\"exclusive-savings\">\n<p>Phone Discounts | Package Deals | Open Box | Close Out | Scratch + Dent | Low Monthly Payments | Free Accessories<\/p><p>Exclusive Huge Savings Just One Call Away! Experts Standing By <a href=\"tel:888-667-4986\">888-667-4986<\/a><\/p>\n<\/div>"
          }
        },
        "6d4a00c6-99ed-42dd-bfd5-1b91c8900dc8": {
          "type": "custom",
          "settings": {
            "code": "<!-- Custom Metafield-->\n<div id=\"custom-variant-meta\">\n    <div class=\" custom-field-block gasType\">\n        {% if product.metafields.custom.option_one_title != blank %}\n        <p class=\"variant__label\">{{ product.metafields.custom.option_field_title }}<\/p>\n        <a href=\"{{ product.metafields.custom.option_one_product.value.url }}\" class=\"custom-variant-btn\">\n        {{ product.metafields.custom.option_one_title }}<\/a>\n        {% endif %}\n\n        {% if product.metafields.custom.option_two_title != blank %}\n        <a href=\"{{ product.metafields.custom.option_two_product.value.url }}\" class=\"custom-variant-btn\">\n        {{ product.metafields.custom.option_two_title }} <\/a>\n        {% endif %}\n\n        {% if product.metafields.custom.option_three_title != blank %}\n        <a href=\"{{ product.metafields.custom.option_three_product.value.url }}\" class=\"custom-variant-btn\">\n        {{ product.metafields.custom.option_three_title }} <\/a>\n        {% endif %}\n    <\/div>\n\n    <div class=\"custom-field-block variantSize\">\n        {% if product.metafields.custom.size_option_title != blank %}\n        <p class=\"variant__label\"> Size <\/p>\n        <a href=\"{{ product.metafields.custom.size_related_product.value.url }}\" class=\"custom-variant-btn\">\n        {{ product.metafields.custom.size_option_title }}<\/a>\n        {% endif %}\n\n        {% if product.metafields.custom.size_two_product_title != blank %}\n        <a href=\"{{ product.metafields.custom.size_two_product.value.url }}\" class=\"custom-variant-btn\">\n            {{ product.metafields.custom.size_two_product_title }}<\/a>\n        {% endif %}\n\n        {% if product.metafields.custom.size_three_title != blank %}\n        <a href=\"{{ product.metafields.custom.size_three_product.value.url }}\" class=\"custom-variant-btn\">\n            {{ product.metafields.custom.size_three_title }}<\/a>\n        {% endif %}\n\n        {% if product.metafields.custom.size_four_title != blank %}\n        <a href=\"{{ product.metafields.custom.size_four_product.value.url }}\" class=\"custom-variant-btn\">\n            {{ product.metafields.custom.size_four_title }}<\/a>\n        {% endif %}\n\n        {% if product.metafields.custom.size_five_title != blank %}\n        <a href=\"{{ product.metafields.custom.size_five_product.value.url }}\" class=\"custom-variant-btn\">\n            {{ product.metafields.custom.size_five_title }}<\/a>\n        {% endif %}\n    <\/div>\n\n    <div class=\"custom-field-block shopOpenBox\">\n        {% if product.metafields.custom.open_box != blank %}\n        <p class=\"variant__label\"> Shop Open Box <\/p>\n        <a href=\"{{ product.metafields.custom.open_box.value.url }}\" class=\"openBox\">\n            {{ product.metafields.custom.open_box.value.title }}<\/a>\n        {% endif %}\n    <\/div>\n<\/div>"
          }
        },
